{
  "name": "ExLinks",
  "version": "2.3.4",
  "author": "Hupo <hupotronic@gmail.com>",
  "description": "Userscript for pretty linkification of E-Hentai & ExHentai links on 4chan & friends",
  "dependencies": {
    "html-minifier": "~0.6.9",
    "uglify-js": "~2.4.15",
	"clean-css": "~3.4.4"
  },
  "devDependencies": {
    "coffee-script": "~1.8.0"
  },
  "scripts": {
    "start": "cake dev",
    "build": "cake build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Hupotronic/ExLinks.git"
  },
  "keywords": [
    "userscript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hupotronic/ExLinks/issues"
  },
  "homepage": "https://hupotronic.github.io/ExLinks/",
  "custom": {
    "namespace": "hupotronic",
    "update_url": "https://github.com/Hupotronic/ExLinks/raw/stable/ExLinks.meta.js",
    "download_url": "https://github.com/Hupotronic/ExLinks/raw/stable/ExLinks.user.js",
    "changelog_url": "https://raw.githubusercontent.com/Hupotronic/ExLinks/master/changelog",
    "targets": [
      "http://boards.4chan.org/*",
      "https://boards.4chan.org/*",
      "http://boards.38chan.net/*",
      "https://archive.moe/*",
      "http://8ch.net/*",
      "https://8ch.net/*"
    ],
    "gm_permissions": [
      "GM_xmlhttpRequest",
      "GM_setValue",
      "GM_getValue",
      "GM_deleteValue",
      "GM_listValues"
    ]
  }
}
