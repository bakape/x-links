{
	"name": "X-links",
	"version": "1.2.1.2",
	"author": "dnsev-h",
	"description": "Making your browsing experience on 4chan and friends more pleasurable",
	"dependencies": {
		"html-minifier": "~0.6.9",
		"uglify-js": "~2.4.15",
		"clean-css": "~3.4.4",
		"complexion": "~0.1.3",
		"complexion-js": "~0.1.5"
	},
	"devDependencies": {},
	"scripts": {},
	"repository": {
		"type": "git",
		"url": "git://github.com/dnsev-h/x-links.git"
	},
	"keywords": [
		"userscript"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dnsev-h/x-links/issues"
	},
	"homepage": "https://dnsev-h.github.io/x-links/",
	"custom": {
		"name_short": "X",
		"namespace": "dnsev-h",
		"settings_prefix": "xlinks-",
		"update_url_base": "https://raw.githubusercontent.com/dnsev-h/x-links/stable/",
		"changelog_url": "https://raw.githubusercontent.com/dnsev-h/x-links/stable/changelog",
		"targets": [
			"http://boards.4chan.org/*",
			"https://boards.4chan.org/*",
			"http://8ch.net/*",
			"https://8ch.net/*",
			"http://desustorage.org/*",
			"https://desustorage.org/*",
			"http://fgts.jp/*",
			"https://fgts.jp/*",
			"http://boards.38chan.net/*",
			"http://forums.e-hentai.org/*",
			"https://forums.e-hentai.org/*"
		],
		"gm_permissions": [
			"GM_xmlhttpRequest",
			"GM_setValue",
			"GM_getValue",
			"GM_deleteValue",
			"GM_listValues"
		]
	}
}
